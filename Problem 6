#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
    int n1,n2,n3,i;
     n1=0, n2=1 ;
     
    cout<<"The next 20 fibonacci numbers are";
    
    for (n3 = i ; i < 20; i++)
    {
    n3 = n1 + n2 ;
    n1 = n2 ;
    n2 = n3;
    cout<<n3<<",";
    }
    _getch();
    return 0;
}
